

class Switch extends React.Component {

    constructor ( props ) {
        super( props );
		
		this.state = {
			isChecked: null
		}
    }
	
	componentWillMount () {
		this.setState( { isChecked: this.props.isChecked } );
	}


    render () {

        return(
            
        );
    }


    _handleChange () {
		this.setState( { isChecked: !this.state.isChecked } );
    }

}

